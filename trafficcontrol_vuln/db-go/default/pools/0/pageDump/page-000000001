SELECT
  s.host_name,
  s.domain_name,
  s.tcp_port,
  c.name as cdn
FROM
  server s
  JOIN type t ON s.type = t.id
  JOIN status st ON st.id = s.status
  JOIN cdn c ON c.id = s.cdn_id
WHERE
  t.name = 'RASCAL'
  AND st.name = (SELECT COALESCE(
    (SELECT p.value
     FROM parameter p
     WHERE p.name = 'tm_query_status_override' AND p.config_file = 'global' LIMIT 1),
