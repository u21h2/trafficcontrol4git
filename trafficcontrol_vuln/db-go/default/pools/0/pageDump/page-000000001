SELECT
	t.name,
	(SELECT ARRAY_AGG(tc.cachegroup ORDER BY tc.cachegroup)
		FROM topology_cachegroup tc
		JOIN cachegroup c ON c.name = tc.cachegroup
		JOIN type ON type.id = c.type
		WHERE t.name = tc.topology
		AND type.name = $1
		) AS nodes
FROM topology t
ORDER BY t.name
`
SELECT
	t.name,
	(SELECT ARRAY_AGG(tc.cachegroup ORDER BY tc.cachegroup)
		FROM topology_cachegroup tc
		JOIN cachegroup c ON c.name = tc.cachegroup
		JOIN type ON type.id = c.type
		WHERE t.name = tc.topology
		AND type.name = $1
		) AS nodes
FROM topology t
ORDER BY t.name
