SELECT
  ds.xml_id,
  tp.name as type,
  ds.protocol,
  c.name as cdn_name,
  c.domain_name as cdn_domain,
  ds.routing_name
FROM
  deliveryservice ds
  JOIN type tp ON tp.id = ds.type
  JOIN cdn c ON c.id = ds.cdn_id
WHERE
  c.name = ANY($1)
